		"@types/pouchdb": "^6.4.2",
		"@types/pouchdb-browser": "^6.1.5",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vite-pwa/sveltekit": "^0.2.7",
		"@zerodevx/svelte-toast": "^0.9.5",
		"autoprefixer": "^10.4.14",
		"daisyui": "^3.9.4",
		"date-picker-svelte": "^2.9.0",
		"eslint": "^8.28.0",
